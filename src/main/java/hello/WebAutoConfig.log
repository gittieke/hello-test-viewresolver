public class WebAutoConfig  extends WebMvcConfigurerAdapter {
	    
	@Bean
    public ViewResolver getViewResolver() {
        InternalResourceViewResolver viewInternalResourceViewResolver = new InternalResourceViewResolver();        
        viewInternalResourceViewResolver.setPrefix("/");
        viewInternalResourceViewResolver.setSuffix("");
        System.out.println("InternalResourceViewResolver");        
        Map<String,Object> m = viewInternalResourceViewResolver.getAttributesMap();        		
        for (Map.Entry<String, Object> entry : m.entrySet()){
        	System.out.println(entry.getKey() + "/" + entry.getValue());
        }        		        	       
        return viewInternalResourceViewResolver;
    }

    @Override
    public void configureDefaultServletHandling(
            DefaultServletHandlerConfigurer configurer) {
        configurer.enable();
    }    
}
 